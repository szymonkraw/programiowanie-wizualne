<Window x:Class="lab05.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab05"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Name="wczyt_sekw" Content="Wczytaj sekwencje" HorizontalAlignment="Left" Margin="10,24,0,0" VerticalAlignment="Top" Height="51" Width="117" Click="wczyt_sekw_Click"/>
        <Button Name="znajdz_wzr" Content="Znajdź wzorce" HorizontalAlignment="Left" Margin="10,110,0,0" VerticalAlignment="Top" Height="51" Width="117" Click="znajdz_wzr_Click"/>
        <ComboBox Name="wybor" HorizontalAlignment="Left" Margin="10,201,0,0" VerticalAlignment="Top" Width="178" Height="51" SelectionChanged="wybor_SelectionChanged" />
        <TextBox Name="szukane_wzorce"  HorizontalAlignment="Left" Margin="10,296,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="178" Height="111"/>
        <RichTextBox Name ="sekwencja_text" HorizontalAlignment="Left" Margin="244,24,0,0" VerticalAlignment="Top" Width="440" Height="353" TextChanged="sekwencja_text_TextChanged"/>
     




    </Grid>
</Window>
